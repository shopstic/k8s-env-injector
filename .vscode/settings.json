{
  "deno.enable": true,
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "files.watcherExclude": {
    "**/target": true
  },
  "deno.suggest.imports.hosts": {
    "https://raw.githubusercontent.com": false
  }
}
